                    <PropertyGroupDescription Converter="{x:Static local:SortHelper.SortByPinYinConverter}"/>
                </CollectionViewSource.GroupDescriptions>
                <CollectionViewSource.SortDescriptions>
                    <componet:SortDescription Direction="Ascending" />
                </CollectionViewSource.SortDescriptions>
            </CollectionViewSource>
        </Grid.Resources>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <DockPanel LastChildFill="False">
            <TextBlock Style="{StaticResource DarkBottomTextBoxStyle}"  Text="动态"  />
            <Button   Style="{StaticResource EmptyButtonWithFocusStyle}" DockPanel.Dock="Right">
                <Grid>
                    <Ellipse Width=" 15" Height=" 15"  Stroke="{Binding RelativeSource={RelativeSource AncestorLevel=1,AncestorType=Button},Path=Foreground }"/>
                </Grid>
            </Button>
