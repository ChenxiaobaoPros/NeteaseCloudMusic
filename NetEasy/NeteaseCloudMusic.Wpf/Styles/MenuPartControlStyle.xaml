                        </ControlTemplate>
                    </Setter.Value>
                </Setter>-->
            </Style>
        </Style.Resources>
        <Setter Property="Width" Value="50"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="Background" >
            <Setter.Value>
                <SolidColorBrush Color="{StaticResource MenuBackGround}"/>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value >
                <ControlTemplate TargetType="{x:Type ct:MenuPartControl}">
                    <DockPanel x:Name="rootLayout" Width="{TemplateBinding Width }"      
                      HorizontalAlignment="Left"   Background="{TemplateBinding Background}" >
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup>
                                <!--关闭状态-->
                                <VisualState x:Name="CloseState">
                                    <Storyboard >
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(StackPanel.Orientation)" Storyboard.TargetName="bottomUser">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.2" Value="{x:Static Orientation.Vertical}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="userInfoPart"
                                                           Storyboard.TargetProperty="(FrameworkElement.Height)">
                                            <DiscreteDoubleKeyFrame KeyTime="0:0:0.2" Value="120"/>
                                        </DoubleAnimationUsingKeyFrames>

                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="OpenState">
                                    <Storyboard >
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="(StackPanel.Orientation)" Storyboard.TargetName="bottomUser">
                                            <DiscreteObjectKeyFrame KeyTime="0:0:0.2" Value="{x:Static Orientation.Horizontal}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="userInfoPart"
                                                           Storyboard.TargetProperty="(FrameworkElement.Height)">
                                            <DiscreteDoubleKeyFrame KeyTime="0:0:0.2" Value="60"/>
                                        </DoubleAnimationUsingKeyFrames>

                                    </Storyboard>
                                </VisualState>

                            </VisualStateGroup>

                        </VisualStateManager.VisualStateGroups>
                        <!--转到VisualState行为-->
                        <i:Interaction.Triggers>
                            <ei:DataTrigger Binding="{Binding IsOpen, RelativeSource={RelativeSource AncestorType={x:Type ct:MenuPartControl}}}" Value="false">

                                <ei:GoToStateAction TargetName="rootLayout"  StateName="CloseState"/>
                            </ei:DataTrigger>
                            <ei:DataTrigger Binding="{Binding IsOpen, RelativeSource={RelativeSource AncestorType={x:Type ct:MenuPartControl}}}" Value="true">

                                <ei:GoToStateAction  TargetName="rootLayout" StateName="OpenState"/>
                            </ei:DataTrigger>
                        </i:Interaction.Triggers>

                        <Grid x:Name="menuIconPart"    Height=" 40" DockPanel.Dock="Top" >
                            <ToggleButton x:Name="menuIcon" Style="{StaticResource menuToggleButtonStyle}"                                       
                          Height=" 20" Width=" 20" Margin="15,0,0,0" 
                          IsChecked="{Binding IsOpen, RelativeSource={RelativeSource AncestorType={x:Type ct:MenuPartControl}}}">
                                <Path HorizontalAlignment="Center" VerticalAlignment="Center"
                    Stretch="Uniform">
                                    <Path.Fill>
                                        <SolidColorBrush Color="{StaticResource  WindowForeDarkColor}"/>
                                    </Path.Fill>
                                    <Path.Data>
                                        <Geometry>M2,15.5L22,15.5 22,17.5 2,17.5 2,15.5z M2,10.5L22,10.5 22,12.5 2,12.5 2,10.5z M2,5.5L22,5.5 22,7.5 2,7.5 2,5.5z</Geometry>
                                    </Path.Data>
                                </Path>

                            </ToggleButton>

                        </Grid>
                        <!--底部用户信息-->
                        <DockPanel  x:Name="userInfoPart"   DataContext="{Binding User}"  DockPanel.Dock="Bottom" Height="120">
                            <Rectangle Height=" 0.8" Fill="Gray" Margin="1,0" Opacity="0.8" DockPanel.Dock="Top"/>
                            <StackPanel Orientation="Vertical"  x:Name="bottomUser">
                                <Button  
                        Command="{TemplateBinding UserImageCommand}"
                       
                                     Padding="4,0,0,0"
                                    Width="80">
                                    <Grid  >
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width=" 40"/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <!--<Ellipse Width="30" Height="{Binding Width, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                                <Ellipse.Fill>
                                                    <ImageBrush Stretch="UniformToFill" ImageSource="{Binding UserImage}"/>
                                                </Ellipse.Fill>
                                            </Ellipse>-->
                                        <!--下面的效果更好？-->
                                        <Image Stretch="Fill"  Width="30" Height="{Binding Width, RelativeSource={RelativeSource Self}}" HorizontalAlignment="Center" VerticalAlignment="Center" Source="{Binding UserImage}">
                                            <Image.Clip>
                                                <EllipseGeometry RadiusX="{Binding Width, RelativeSource={RelativeSource AncestorType={x:Type Image}},Converter={x:Static converter:ValueConverterFactory.HalfDoubleConverter} }" RadiusY="{Binding RadiusX, RelativeSource={RelativeSource Self}}" >
                                                    <EllipseGeometry.Center>
                                                        <MultiBinding Converter="{x:Static converter:MultiValueConverterFactory.Coordinate2PointConverter}">
                                                            <Binding Path="RadiusX" RelativeSource="{RelativeSource Self}"></Binding>
                                                            <Binding Path="RadiusY" RelativeSource="{RelativeSource Self}"></Binding>

                                                        </MultiBinding>
                                                    </EllipseGeometry.Center>
                                                </EllipseGeometry>
                                            </Image.Clip>
                                        </Image>
                                        <Label HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Grid.Column="1"
                                              Visibility="{Binding RelativeSource={RelativeSource AncestorType={x:Type ct:MenuPartControl}},Path=IsOpen,Converter={x:Static converter:ValueConverterFactory.Boolean2VisibilityConverter}  }" 
                                              >
                                            <TextBlock Text= "{Binding UserName,TargetNullValue=未知用户 }" 
                                                       FontFamily="{StaticResource WindowMainFont}"
                                                       FontSize="12"
                                                       TextWrapping="NoWrap" TextTrimming="WordEllipsis" ToolTip="{Binding Text, RelativeSource={RelativeSource Self}}"   >
                                                <TextBlock.Foreground>
                                                    <SolidColorBrush Color="{StaticResource WindowForeDarkColor}"/>
                                                </TextBlock.Foreground>
                                            </TextBlock>
                                        </Label>
                                    </Grid>
