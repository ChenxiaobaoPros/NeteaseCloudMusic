        <ResourceDictionary Source="/NeteaseCloudMusic.Wpf;component/Styles/ToggleButtonStyle.xaml"/>
        <ResourceDictionary Source="/NeteaseCloudMusic.Wpf;component/Styles/ListBoxDataStyle.xaml"/>

    </ResourceDictionary.MergedDictionaries>

    <Style TargetType="{x:Type ct:MenuPartControl}" x:Key="defaultStyle">
        <Style.Resources>
            <Style TargetType="{x:Type Button}" BasedOn="{StaticResource EmptyButtonStyle}">
                <Setter Property="Background" Value="Transparent"></Setter>
                <Setter Property="BorderThickness" Value=" 0"/>
                <Setter Property="BorderBrush" Value="Transparent"/>
                <Setter Property="Padding" Value=" 10"/>
                <!--<Setter Property="HorizontalContentAlignment" Value="Stretch" />
                <Setter Property="VerticalContentAlignment" Value="Stretch" />-->
                <Setter Property="Height" Value=" 40"/>
                <Setter Property="Width" Value=" 40"/>
